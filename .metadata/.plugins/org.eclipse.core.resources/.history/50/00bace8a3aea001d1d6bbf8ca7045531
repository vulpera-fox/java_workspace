package day15.api.io.file;
import java.io.*;

public class CreateFileEx {

	public static void main(String[] args) {

		/*
		 * File 클래스
		 * 외부경로에 폴더나 단순한 파일을 생성하거나 삭제할 수 있
		 */

		String path = "/Users/uwu/file/20230504";


		try {

			File file = new File(path);
			file.mkdir(); // makedirectory	

		} catch (Exception e) {
			e.printStackTrace();
		}

	}

}
