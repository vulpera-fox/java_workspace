package quiz19;

import java.util.ArrayList;
import java.util.List;

public class ListQuiz01 {

	public static void main(String[] args) {
		
		/*
		 *  1. list에 1~20까지 순서대로 값을 저장
		 *  2. list의 값을 순서대로 출력
		 */
		
		List<Integer> list = new ArrayList<>();
		
		for ( int i = 1; i <= 20; i++ ) {
			list.add(i);
		}

		for ( int i = 1; i <= 20; i++ ) {
			System.out.print(list.get(i-1) + " ");
		}
		
		

	}

}
