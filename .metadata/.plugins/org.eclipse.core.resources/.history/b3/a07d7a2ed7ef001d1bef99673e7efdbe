package coding;

import java.util.*;
import java.util.stream.Stream;

class Solution {
    public String solution(String[] id_pw, String[][] db) {
    	
    	for ( int i = 0; i < db.length; i++ ) {
    		if ( !id_pw[0].equals(db[i][0]) ){
    			return "fail";
    		} if ( !id_pw[1].equals(db[i][1]) ) {
    			return "wrong pw";
    		}
    	}
    	return "login";
}
}