package coding;

import java.util.Arrays;


class Solution {
    public int solution(String[] spell, String[] dic) {
    	int answer = 0;
        for ( int i = 0; i < spell.length; i++ ){
            for ( int j = 0; j < dic.length; j++ ){
                dic[j].replace(spell[i],"");
            }
        }
        for ( int i = 0; i < spell.length; i++ ) {
        	if ( dic[i].equals("") ) {
        		answer = 1;
        	} else {
        		answer = 2;
        	}
        }
        return answer;
    }
}