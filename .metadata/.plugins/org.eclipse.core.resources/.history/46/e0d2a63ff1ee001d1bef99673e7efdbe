package day17.api.thread;

public class ThreadTest02 extends Thread {

	@Override
	public void run() {
		
		for ( int i =1; i <= 10; i++ ) {
			System.out.println("쓰레드를 상속받은 클래스 실행: " + i);
			
			try {
			sleep(1000); // 상속받은 sleep()	
			} catch (Exception e) {

			}
			
			System.out.println("메인스레드 종료");
		}
	}
	
	

}
